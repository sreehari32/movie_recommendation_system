  },
  {
   "cell_type": "code",
   "execution_count": 284,
   "metadata": {},
   "outputs": [],
   "source": [
    "#function to extract top 3 cast\n",
    "import ast\n",
    "def extraction_1(obj):\n",
    "    lis=[]\n",
    "    j=0\n",
    "    for i in ast.literal_eval(obj):\n",
    "        if j<3:\n",
    "            lis.append(i['name'])\n",
    "            j+=1\n",
    "    return lis"
   ]
  },
